{
  "viewport": {
    "min": {
      "value": "360",
      "type": "sizing"
    },
    "max": {
      "value": "1920",
      "type": "sizing"
    },
    "width": {
      "value": "1280",
      "type": "sizing"
    }
  },
  "test": {
    "min": {
      "value": "16",
      "type": "sizing"
    },
    "max": {
      "value": "32",
      "type": "sizing"
    },
    "a1": {
      "value": "{viewport.min}*{test.max}",
      "type": "dimension"
    },
    "a2": {
      "value": "{viewport.max}*{test.min}",
      "type": "dimension"
    },
    "r1": {
      "value": "{test.a1} - {test.a2}",
      "type": "other"
    },
    "r2": {
      "value": "{viewport.min}-{viewport.max}",
      "type": "other"
    },
    "r": {
      "value": "(({viewport.min}*{test.max}) - ({viewport.max}*{test.min})/({viewport.min}-{viewport.max})",
      "type": "other"
    },
    "v": {
      "value": "100 * ({test.max} - {test.min}) / ({viewport.max} - {viewport.min})",
      "type": "other"
    },
    "vw": {
      "value": "{test.v} * {vw}",
      "type": "dimension"
    },
    "bar": {
      "value": " ((({viewport.min}*{test.max})-({viewport.max}*{test.min})) / ({viewport.min}-{viewport.max})) ",
      "type": "dimension"
    },
    "fluid": {
      "value": "(((100 * ({test.max}-{test.min}) / ({viewport.max}-{viewport.min})) * {vw}))",
      "type": "dimension"
    },
    "not-breaking-expr": {
      "value": "{test.bar} + {test.fluid}",
      "type": "dimension",
      "description": "resolves great in token studio and without px applied in SD, not working with px applied in SD"
    },
    "breaking-expr": {
      "value": "((({viewport.min}*{test.max})-({viewport.max}*{test.min})) / ({viewport.min}-{viewport.max})) + (((100 * ({test.max}-{test.min}) / ({viewport.max}-{viewport.min})) * {vw}))",
      "type": "dimension",
      "description": "breaks expr-eval if i try to do it inline to prevent the px problem"
    },
    "clamp": {
      "value": "clamped({test.min}, ((({viewport.min}*{test.max})-({viewport.max}*{test.min})) / ({viewport.min}-{viewport.max})) + (((100 * ({test.max}-{test.min}) / ({viewport.max}-{viewport.min})) * {vw})) ,{test.max})",
      "type": "dimension",
      "description": "Test new function for clamped in SD https://github.com/tokens-studio/figma-plugin/pull/1657"
    }
  },
  "vw": {
    "value": "({viewport.width}*0.01)",
    "type": "unit"
  }
}